<table>
{% for a in acteurs %}
	<tr>
		<td>{{ a.nom }}</td>
		<td>{{ a.prenom }}</td>
		<td>{{ a.dateNaissance|date('d/m/Y') }}</td>
		<td>{{ a.sexe }}</td>
		<td><a href="{{ path('demoapp_acteur_modifier', { 'id': a.id }) }}">{{"acteur.Modifier"|trans}}</a></td>
		<td><a href="{{ path('demoapp_acteur_supprimer', { 'id': a.id }) }}">{{"acteur.Supprimer"|trans}}</a></td>
	</tr>
{% else %}
	<tr><td>{{"acteur.aucunActeurTrouve"|trans}}</td></tr>
{% endfor %}
</table>